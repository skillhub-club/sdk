#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/keyu.yuan/Downloads/skillhubs/node_modules/.pnpm/tsup@8.5.1_@swc+core@1.15.8_jiti@1.21.7_postcss@8.5.6_tsx@4.21.0_typescript@5.9.3_yaml@2.8.2/node_modules/tsup/dist/node_modules:/Users/keyu.yuan/Downloads/skillhubs/node_modules/.pnpm/tsup@8.5.1_@swc+core@1.15.8_jiti@1.21.7_postcss@8.5.6_tsx@4.21.0_typescript@5.9.3_yaml@2.8.2/node_modules/tsup/node_modules:/Users/keyu.yuan/Downloads/skillhubs/node_modules/.pnpm/tsup@8.5.1_@swc+core@1.15.8_jiti@1.21.7_postcss@8.5.6_tsx@4.21.0_typescript@5.9.3_yaml@2.8.2/node_modules:/Users/keyu.yuan/Downloads/skillhubs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/keyu.yuan/Downloads/skillhubs/node_modules/.pnpm/tsup@8.5.1_@swc+core@1.15.8_jiti@1.21.7_postcss@8.5.6_tsx@4.21.0_typescript@5.9.3_yaml@2.8.2/node_modules/tsup/dist/node_modules:/Users/keyu.yuan/Downloads/skillhubs/node_modules/.pnpm/tsup@8.5.1_@swc+core@1.15.8_jiti@1.21.7_postcss@8.5.6_tsx@4.21.0_typescript@5.9.3_yaml@2.8.2/node_modules/tsup/node_modules:/Users/keyu.yuan/Downloads/skillhubs/node_modules/.pnpm/tsup@8.5.1_@swc+core@1.15.8_jiti@1.21.7_postcss@8.5.6_tsx@4.21.0_typescript@5.9.3_yaml@2.8.2/node_modules:/Users/keyu.yuan/Downloads/skillhubs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsup/dist/cli-node.js" "$@"
else
  exec node  "$basedir/../tsup/dist/cli-node.js" "$@"
fi
