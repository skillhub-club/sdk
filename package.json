{
  "name": "@skill-hub/sdk",
  "version": "1.0.1",
  "description": "Official SDK for SkillHub API - Types and client for AI agent skills",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types.mjs",
      "require": "./dist/types.js",
      "types": "./dist/types.d.ts"
    },
    "./client": {
      "import": "./dist/client.mjs",
      "require": "./dist/client.js",
      "types": "./dist/client.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/types.ts src/client.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts src/types.ts src/client.ts --format cjs,esm --dts --watch",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "skillhub",
    "claude",
    "ai",
    "skills",
    "sdk",
    "api",
    "mcp"
  ],
  "author": "SkillHub",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/skillhub-club/skillhub-sdk"
  },
  "homepage": "https://skillhub.club",
  "bugs": {
    "url": "https://github.com/skillhub-club/skillhub-sdk/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.7.0"
  }
}
